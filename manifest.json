{
    "manifest_version": 3,
    "name": "Dark Pinterst",
    "desription": "Making pinterest theme dark in your browser.",
    "version": "0.0.1",
    "author": "slurry",
    "action":{
        "default_popup": "html/popup.html"    
    },
    "content_scripts": [
        {
            "css":["css/style.css"],
            "matches":[
                "https://ru.pinterest.com/",
                "https://pinterest.com/"
            ]
        }
    ],
    "permissions":[
        "activeTab"
    ]
}